/* pages/my/setting/setting.wxss */
page{
  background: #F0F0F0;
}
.list{
  margin-top: .8rem;
  background: #fff;
  padding: 0 .5rem;
}
.list .line{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: .5rem 0;
  border-bottom: 1rpx solid #F0F0F0;
}
.list .line view{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list .line image{
  width: 3rem;
  height: 3rem;
}
.list .iconfont{
  display: flex;
  font-size: 1rem;
}
.nickname{
  text-align: right;
}